    <storeData>
        <name>$l10n_horschSpreader</name>
        <specs>
            <workingWidth>36.0</workingWidth>
        </specs>
        <functions>
            <function>$l10n_function_fertilizer</function>
        </functions>
        <image>horsch/rauch10000/store_horschRauch10000.png</image>
        <price>51000</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>HORSCH</brand>
        <category>fertilizerSpreaders</category>
        <vertexBufferMemoryUsage>1359132</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>271272</indexBufferMemoryUsage>
        <textureMemoryUsage>31108056</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_fertilizerSpreader</typeDesc>
        <filename>horsch/rauch10000/rauch10000.i3d</filename>
        <size width="4" length="5.5"/>
        <speedLimit value="20"/>
        <components>
            <component centerOfMass="0 0.7 0.1" solverIterationCount="10" mass="2200"/>
        </components>
        <schemaOverlay attacherJointPosition="0 0">
            <default name="DEFAULT_IMPLEMENT"/>
            <turnedOn name="DEFAULT_IMPLEMENT_ON"/>
            <selected name="DEFAULT_IMPLEMENT_SELECTED"/>
            <turnedOnSelected name="DEFAULT_IMPLEMENT_SELECTED_ON"/>
        </schemaOverlay>
    </base>

    <wheels>
        <wheelConfigurations>
            <wheelConfiguration name="$l10n_configuration_valueWheelNarrow" price="0">
                <wheels>
                    <wheel hasTireTracks="true" hasParticles="true">
                        <physics rotSpeed="1" restLoad="0.4" repr="wheelBack" driveNode="driveNodeLeft" radius="0.4727" width="0.5" mass="0.2" initialCompression="20" suspTravel="0.15" spring="15" damper="30" frictionScale="2.5"/>
                        <tire tireTrackAtlasIndex="7" isCareWheel="true" />
                    </wheel>
                    <wheel>
                        <physics rotSpeed="1" restLoad="0.8" repr="wheelFrontLeft" radius="0.4727" width="0.5" mass="0.2" initialCompression="20" suspTravel="0.15" spring="15" damper="30" frictionScale="2" tireType="crawler"/>
                        <tire isCareWheel="true" />
                    </wheel>
                    <wheel hasTireTracks="true" hasParticles="true">
                        <physics rotSpeed="1" restLoad="0.4" repr="wheelBackRight" driveNode="driveNodeRight" radius="0.4727" width="0.5" mass="0.2" initialCompression="20" suspTravel="0.15" spring="15" damper="30" frictionScale="2.5"/>
                        <tire tireTrackAtlasIndex="7" isCareWheel="true" />
                    </wheel>
                    <wheel>
                        <physics rotSpeed="1" restLoad="0.8" repr="wheelFrontRight" radius="0.4727" width="0.5" mass="0.2" initialCompression="20" suspTravel="0.15" spring="15" damper="30" frictionScale="2" tireType="crawler"/>
                        <tire isCareWheel="true" />
                    </wheel>
                </wheels>
                <crawlers>
                    <crawler filename="$data/shared/wheels/crawlers/horsch/camsoTracks.xml" linkNode="crawlerLeft"  wheelIndex="2" isLeft="true"/>
                    <crawler filename="$data/shared/wheels/crawlers/horsch/camsoTracks.xml" linkNode="crawlerRight" wheelIndex="4" isLeft="false"/>
                </crawlers>
            </wheelConfiguration>
        </wheelConfigurations>
    </wheels>

    <attachable>
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" jointType="trailerLow" attacherHeight="0.55"/>
        </inputAttacherJoints>
        <support animationName="moveSupport"/>
        <brakeForce>0.4</brakeForce>
        <airConsumer usage="0.3"/>
    </attachable>

    <connectionHoses>
        <hose inputAttacherJointIndices="1" type="hydraulicIn"  node="hydraulicIn01"  length="1.1" diameter="0.02">
            <objectChange node="hydraulicIn01_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicOut" node="hydraulicOut01" length="1.1" diameter="0.02">
            <objectChange node="hydraulicOut01_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>

        <hose inputAttacherJointIndices="1" type="hydraulicIn"  node="hydraulicIn02"  length="1.1" diameter="0.02">
            <objectChange node="hydraulicIn02_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicOut" node="hydraulicOut02" length="1.1" diameter="0.02">
            <objectChange node="hydraulicOut02_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicOut" node="hydraulicOut03" length="1.1" diameter="0.02">
            <objectChange node="hydraulicOut03_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
    </connectionHoses>

    <powerConsumer ptoRpm="400" neededMaxPtoPower="10"/>

    <lights>
      <sharedLight linkNode="Light_03_02"  filename="$data/shared/assets/lights/lizard/rear2ChamberLight_05_right.xml" />
      <sharedLight linkNode="Light_03_01"  filename="$data/shared/assets/lights/lizard/rear2ChamberLight_05_left.xml" />
      <sharedLight linkNode="frontLightCone_01_02"  filename="$data/shared/assets/lights/lizard/frontLightCone_01.xml" />
      <sharedLight linkNode="frontLightCone_01_01"  filename="$data/shared/assets/lights/lizard/frontLightCone_01.xml" />
      <sharedLight linkNode="redRectangle_01_02"  filename="$data/shared/assets/reflectors/lizard/redRectangle_01.xml" />
      <sharedLight linkNode="redRectangle_01_01"  filename="$data/shared/assets/reflectors/lizard/redRectangle_01.xml" />
        <realLights>
            <high>
                <light node="backLightsHigh1" lightTypes="0" />
                <brakeLight node="backLightsHigh1" />
                <turnLightLeft node="turnLightLeftBack" />
                <turnLightRight node="turnLightRightBack" />
            </high>
        </realLights>
    </lights>

    <workAreas>
        <workArea type="sprayer" functionName="processSprayerArea" disableBackwards="false" requiresGroundContact="false" >
            <area startNode="startIndex" widthNode="widthIndex" heightNode="heightIndex"/>
        </workArea>
    </workAreas>

    <sprayer fillUnitIndex="1">
        <usageScales workingWidth="36" scale="1" />
        <effects>
            <effectNode effectNode="effectNode" delay="0" materialType="spreader" materialTypeId="1" />
        </effects>
        <animationNodes>
            <animationNode node="spreaderLeft"  rotSpeed="-1500" turnOnFadeTime="0.5" turnOffFadeTime="3" />
            <animationNode node="spreaderRight" rotSpeed="1500" turnOnFadeTime="0.5" turnOffFadeTime="3" />
        </animationNodes>
    </sprayer>

    <turnOnVehicle>
        <sounds>
            <work template="DEFAULT_SPREADER" linkNode="horschRauch10000_main_component1" />
        </sounds>
    </turnOnVehicle>

    <foldable>
        <foldingParts startMoveDirection="1">
            <foldingPart animationName="folding" speedScale="1"/>
        </foldingParts>
    </foldable>

    <animations>
        <animation name="toggleCover">
            <part node="door" startTime="0" endTime="1.5" startRot="0 0 -90" endRot="0 0 0"/>
        </animation>
        <animation name="moveSupport">
            <part node="supportPart" startTime="0.5" endTime="1" startRot="720 0 0" endRot="0 0 0"/>
            <part node="supportArm2" startTime="0.5" endTime="1" startTrans="0 0.184 0" endTrans="0 0 0"/>
            <part node="supportArm" startTime="0" endTime="0.5" startRot="90 0 0" endRot="0 0 0"/>
            <part node="supportCol" startTime="0" endTime="0.2" startTrans="0 0.5 0" endTrans="0 -0.05 0"/>
        </animation>
        <animation name="folding">
            <part node="axisLeft" startTime="0" endTime="2.0" startTrans="0 0 0" endTrans="-0.25 0 0"/>
            <part node="axisRight" startTime="0" endTime="2.0" startTrans="0 0 0" endTrans="0.25 0 0"/>
        </animation>
    </animations>

    <cylindered>
        <movingTools>
            <movingTool node="door">
                <dependentPart node="hydraulicDoor"/>
            </movingTool>
            <movingTool node="axisLeft" wheelIndices="1 2" playSound="true" />
            <movingTool node="axisRight" wheelIndices="3 4"/>
        </movingTools>
        <movingParts>
            <movingPart node="hydraulicDoor" referencePoint="hydraulicDoorRef" referenceFrame="hydraulicDoorRefFrame" playSound="true">
                <translatingPart node="hydraulicDoorPunch"/>
            </movingPart>
            <movingPart node="wheelBack" referencePoint="wheelFrontLeft" referenceFrame="horschRauch10000_main_component1" isActiveDirty="true" />
            <movingPart node="wheelBackRight" referencePoint="wheelFrontRight" referenceFrame="horschRauch10000_main_component1" isActiveDirty="true" />
        </movingParts>
        <sounds>
            <hydraulic template="DEFAULT_HYDRAULIC_SOUND" linkNode="horschRauch10000_main_component1"/>
        </sounds>
    </cylindered>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits>
                    <unloading width="24" offset="0 0 0" />
                    <fillUnit unit="$l10n_unit_literShort" fillTypeCategories="spreader" capacity="10000">
                        <exactFillRootNode node="exactFillRootNode"/>
                    </fillUnit>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <fillVolume>
        <fillVolumeConfigurations>
            <fillVolumeConfiguration>
                <volumes>
                    <volume node="fillVolume" maxDelta="0.2" fillUnitIndex="1"/>
                </volumes>
            </fillVolumeConfiguration>
        </fillVolumeConfigurations>
    </fillVolume>

    <cover>
        <coverConfigurations>
            <coverConfiguration>
                <cover openAnimation="toggleCover" openOnBuy="true" fillUnitIndices="1"/>
            </coverConfiguration>
        </coverConfigurations>
    </cover>

    <wearable wearDuration="480" workMultiplier="5" fieldMultiplier="2"/>
    <washable dirtDuration="90" washDuration="1" workMultiplier="4" fieldMultiplier="2"/>

    <ai>
        <needsLowering value="false"/>
        <areaMarkers leftNode="aiMarkerLeft" rightNode="aiMarkerRight" backNode="aiMarkerBack"/>
        <sizeMarkers leftNode="sizeMarkerLeft" rightNode="sizeMarkerRight" backNode="sizeMarkerBack"/>
        <turningRadiusLimitation radius="7"/>
        <allowTurnBackward value="false"/>
        <toolReverserDirectionNode node="sizeMarkerBack"/>
    </ai>

    <foliageBending>
        <bendingNode minX="-0.4" maxX="0.4" minZ="0.5" maxZ="2.2" yOffset="0.65" />
        <bendingNode minX="-1.3" maxX="1.3" minZ="-1.8" maxZ="0.5" yOffset="0.65" />
        <bendingNode minX="1.25" maxX="1.9" minZ="-2.4" maxZ="0.5" yOffset="0.45" node="axisLeft" />
        <bendingNode minX="-1.9" maxX="-1.25" minZ="-2.4" maxZ="0.5" yOffset="0.45" node="axisRight" />
    </foliageBending>

    <baseMaterial>
        <material name="rauch10000_mat" baseNode="horsch_vis">
            <shaderParameter name="colorMat0" value="HORSCH_RED1"/>
        </material>
    </baseMaterial>

