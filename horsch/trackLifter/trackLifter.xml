                        <tire tireType="crawler" tireTrackAtlasIndex="7" isCareWheel="false" />
                    </wheel>
                    <wheel>
                        <physics rotSpeed="1" tipOcclusionAreaGroupId="1" restLoad="6.5" repr="wheelFrontLeft"                            radius="0.65" width="0.85" mass="1.2" initialCompression="40" suspTravel="0.2" spring="100" damper="80" frictionScale="12"/>
                    </wheel>
                    <wheel hasTireTracks="true" hasParticles="true">
                        <physics rotSpeed="1" tipOcclusionAreaGroupId="1" restLoad="6.5" repr="wheelBackRight" driveNode="driveNodeRight" radius="0.65" width="0.85" mass="1.2" initialCompression="40" suspTravel="0.2" spring="100" damper="80" frictionScale="12"/>
