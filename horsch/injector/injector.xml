    <storeData>
        <name>$l10n_horschInjector</name>
        <specs>
            <neededPower>50</neededPower>
            <workingWidth>12</workingWidth>
            <combination>$l10n_horschSlurryTank21000</combination>
        </specs>
        <functions>
            <function>$l10n_function_slurrySpreaderTool</function>
        </functions>
        <image>horsch/injector/store_horschInjector.png</image>
        <price>32000</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>HORSCH</brand>
        <category>slurryTanks</category>
        <shopHeight>5</shopHeight>
        <shopTranslationOffset>0 0 0</shopTranslationOffset>
        <vertexBufferMemoryUsage>2074576</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>400452</indexBufferMemoryUsage>
        <textureMemoryUsage>6815952</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_injector</typeDesc>
        <filename>horsch/injector/injector.i3d</filename>
        <size width="4" length="4" lengthOffset="0.5"/>
        <speedLimit value="17"/>
        <components>
            <component centerOfMass="0 0.2 0.25" solverIterationCount="10" mass="1100"/>
            <component centerOfMass="0 -0.18 0.1" solverIterationCount="10" mass="400"/>
            <component centerOfMass="0 -0.18 0.1" solverIterationCount="10" mass="400"/>
            <joint component1="1" component2="2" node="leftArmJoint" rotLimit="0 0 0" transLimit="0 0 0"/>
            <joint component1="1" component2="3" node="rightArmJoint" rotLimit="0 0 0" transLimit="0 0 0"/>
        </components>
        <schemaOverlay attacherJointPosition="0 0">
            <default name="DEFAULT_IMPLEMENT"/>
            <turnedOn name="DEFAULT_IMPLEMENT_ON"/>
            <selected name="DEFAULT_IMPLEMENT_SELECTED"/>
            <turnedOnSelected name="DEFAULT_IMPLEMENT_SELECTED_ON"/>
        </schemaOverlay>
    </base>

    <wheels>
        <wheelConfigurations>
            <wheelConfiguration name="$l10n_configuration_valueDefault" price="0">
                <wheels>
                    <wheel>
                        <physics rotSpeed="0" restLoad="0.8" repr="wheelLeft" forcePointRatio="0.2" width="0.2" radius="0.17" mass="0.10" initialCompression="40" suspTravel="0.05" spring="32" damper="50" isSynchronized="false"/>
                    </wheel>
                    <wheel>
                        <physics rotSpeed="0" restLoad="0.8" repr="wheelRight" forcePointRatio="0.2" width="0.2" radius="0.17" mass="0.10" initialCompression="40" suspTravel="0.05" spring="32" damper="50" isSynchronized="false"/>
                    </wheel>
                    <wheel>
                        <physics rotSpeed="0" restLoad="0.3" repr="wheelLeftComponent" forcePointRatio="0.2" width="0.2" radius="0.444" mass="0.05" initialCompression="40" suspTravel="0.05" spring="13" damper="50" rotationDamping="0.002"/>
                    </wheel>
                    <wheel>
                        <physics rotSpeed="0" restLoad="0.3" repr="wheelRightComponent" forcePointRatio="0.2" width="0.2" radius="0.444" mass="0.05" initialCompression="40" suspTravel="0.05" spring="13" damper="50" rotationDamping="0.002"/>
                    </wheel>
                </wheels>
            </wheelConfiguration>
        </wheelConfigurations>
        <dynamicallyLoadedWheels>
            <dynamicallyLoadedWheel linkNode="wheelDynLeftArm" filename="$data/shared/wheels/trelleborg/T421/420_55R17.xml" isLeft="true"/>
            <dynamicallyLoadedWheel linkNode="wheelDynRightArm" filename="$data/shared/wheels/trelleborg/T421/420_55R17.xml" isLeft="false"/>
        </dynamicallyLoadedWheels>

        <hubs>
            <color0>HORSCH_RED1</color0>
            <hub linkNode="wheelDynLeftArm" filename="$data/shared/wheels/hubs/hub_n10_5.xml" isLeft="true"  scale="0.2 0.2 0.2" />
            <hub linkNode="wheelDynRightArm" filename="$data/shared/wheels/hubs/hub_n10_5.xml" isLeft="false"  scale="0.2 0.2 0.2" />
        </hubs>
    </wheels>

    <speedRotatingParts>
      <speedRotatingPart node="horschInjector_horsch_vis_Left_Arm2Discs_discs"       radius="0.2" groundReferenceNodeIndex="1" foldMinLimit="0" foldMaxLimit="0.15" />
      <speedRotatingPart node="horschInjector_horsch_vis_LeftArmDiscs_discs"         radius="0.2" groundReferenceNodeIndex="1" foldMinLimit="0" foldMaxLimit="0.15" />
      <speedRotatingPart node="horschInjector_horsch_vis_RightArm_CenterDiscs_discs" radius="0.2" groundReferenceNodeIndex="1" foldMinLimit="0" foldMaxLimit="0.15" />
      <speedRotatingPart node="horschInjector_horsch_vis_rightArm_Discs_discs"       radius="0.2" groundReferenceNodeIndex="1" foldMinLimit="0" foldMaxLimit="0.15" />
      <speedRotatingPart node="wheelDynLeftArm" wheelIndex="3"/>
      <speedRotatingPart node="wheelDynRightArm" wheelIndex="4"/>
    </speedRotatingParts>

    <attachable allowFoldingWhileLowered="false">
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" jointType="implement" topReferenceNode="topReferenceNode" lowerRotLimitScale="0 0 0" upperRotLimitScale="0 0 0" allowsLowering="true">
                <distanceToGround lower="0.5" upper="1"/>
            </inputAttacherJoint>
        </inputAttacherJoints>
        <support animationName="moveSupport"/>
    </attachable>

    <connectionHoses>
        <hose inputAttacherJointIndices="1" type="TOOL_CONNECTOR_TOP_RIGHT" node="topRightPipeMiddle" length="2" diameter="0.16" straighteningFactor="1" isTwoPointHose="true">
            <objectChange node="topRightPipeStart" visibilityActive="true" visibilityInactive="false"/>
        </hose>

        <localHose>
            <hose type="TOOL_CONNECTOR_TOP_RIGHT"  node="topRightPipeStart"  length="2" diameter="0.16" minCenterPointAngle="30" isWorldSpaceHose="false" isTwoPointHose="true" straighteningFactor="1"/>
            <target type="TOOL_CONNECTOR_TOP_RIGHT" node="topRightPipeMiddle" />
        </localHose>
    </connectionHoses>

    <powerConsumer forceNode="horschInjector_main_component1" maxForce="15"/>

    <lights>
      <sharedLight linkNode="Light_03"          filename="$data/shared/assets/lights/lizard/rear2ChamberLight_05_left.xml" />
      <sharedLight linkNode="Light_03_01"       filename="$data/shared/assets/lights/lizard/rear2ChamberLight_05_right.xml" />
      <sharedLight linkNode="redTriangle_02"    filename="$data/shared/assets/reflectors/lizard/redTriangle_02.xml" />
      <sharedLight linkNode="redTriangle_02_01" filename="$data/shared/assets/reflectors/lizard/redTriangle_02.xml" />
        <realLights>
            <high>
