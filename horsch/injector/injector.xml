          <part node="hydraulicRightOuterArmPunch" startTime="3.0" endTime="7.0"  startTrans="0 0 0" endTrans="0 0 0.6" />

          <part node="leftArmJoint"     startTime="7.0" endTime="10.0" startRot="0 0 0" endRot="0 0 90" />
          <part node="rightArmJoint"     startTime="7.0" endTime="10.0" startRot="0 0 0" endRot="0 0 -90" />
      </animation>
        <animation name="moveSupport">
            <part node="supportCol" startTime="0" endTime="0.5" startTrans="0 0.4 0" endTrans="0 0 0"/>
            <part node="decals4_decal" startTime="0.4" endTime="0.45" visibility="false"/>
            <part node="decals4_decal" startTime="0.45" endTime="0.5" visibility="true"/>
        </animation>
    </animations>

    <cylindered>
        <movingTools>
            <movingTool node="leftArmJoint" playSound="true">
                <componentJoint index="1"/>
            </movingTool>
            <movingTool node="rightArmJoint" playSound="true">
