            </movingPart>
            <movingPart node="hydraulicRightArmPipeArm" referencePoint="hydraulicRightArmPipeArmRef" referenceFrame="hydraulicRightArmPipeArmRef" playSound="true">
                <translatingPart node="hydraulicRightArmPipeArmPunch"/>
            </movingPart>
            <movingPart node="hydraulicLeftOuterArmPart" referencePoint="hydraulicLeftOuterArmRef" referenceFrame="hydraulicLeftOuterArmPartRefFrame" scaleZ="true"/>
            <movingPart node="hydraulicRightOuterArmPart" referencePoint="hydraulicRightOuterArmRef" referenceFrame="hydraulicRightOuterArmPartRefFrame" scaleZ="true"/>
        </movingParts>
        <sounds>
            <hydraulic template="DEFAULT_HYDRAULIC_SOUND" linkNode="horschInjector_main_component1"/>
        </sounds>
    </cylindered>

