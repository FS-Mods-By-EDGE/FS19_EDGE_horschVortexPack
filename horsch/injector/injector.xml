            <movingPart node="hydraulicLeftArmPipeArm" referencePoint="hydraulicLeftArmPipeArmRef" referenceFrame="hydraulicLeftArmPipeArmRef" playSound="true">
                <translatingPart node="hydraulicLeftArmPipeArmPunch"/>
            </movingPart>
            <movingPart node="rightArm" referencePoint="rightArmRef" referenceFrame="rightArmRef" isActiveDirty="true">
