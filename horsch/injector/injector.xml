                <turnLightLeft node="turnLightLeftBack"/>
                <turnLightRight node="turnLightRightBack"/>
                <brakeLight node="backLightsHigh2"/>
            </high>
        </realLights>
    </lights>

    <groundReferenceNodes>
        <groundReferenceNode node="groundRefNodeMiddle" threshold="0.2" depthNode="depthNode"/>
    </groundReferenceNodes>

    <workAreas>
        <workArea type="sprayer" functionName="processSprayerArea" disableBackwards="false">
            <area startNode="areaStart" widthNode="areaWidth" heightNode="areaHeight"/>
            <groundReferenceNode index="1"/>
        </workArea>
    </workAreas>

    <sprayer activateOnLowering="true">
        <usageScales workingWidth="12" scale="1" />
        <sounds>
            <work template="DEFAULT_SOWING_WORK" linkNode="0>" />
        </sounds>
    </sprayer>

    <foldable>
        <foldingParts startMoveDirection="1" foldMiddleAnimTime="0.15" turnOnFoldMinLimit="0" turnOnFoldMaxLimit="0.02">
            <foldingPart animationName="folding" speedScale="1"/>
        </foldingParts>
    </foldable>

    <animations>
      <animation name="folding">
          <part componentJointIndex="1" startTime="0.0" endTime="1.0" startRotLimit="0 0 20" endRotLimit="0 0 0" />
          <part componentJointIndex="2" startTime="0.0" endTime="1.0" startRotLimit="0 0 20" endRotLimit="0 0 0" />

          <part node="leftArmPipeArm"   startTime="2.0" endTime="3.0"  startRot="0 0 0" endRot="0 0 -60" />
          <part node="rightArmPipeArm"   startTime="2.0" endTime="3.0"  startRot="0 0 0" endRot="0 0 60" />
          <part node="leftArm2"   startTime="3.0" endTime="7.0"  startRot="0 0 0" endRot="-179.9 0 0" />
          <part node="colArmLeftPart"       startTime="3.0" endTime="7.0"  startRot="0 0 0" endRot="0 0 179.9" />
          <part node="rightArm2"   startTime="3.0" endTime="7.0"  startRot="0 0 0" endRot="-179.9 0 0" />
          <part node="colArmRightPart"       startTime="3.0" endTime="7.0"  startRot="0 0 0" endRot="0 0 -179.9" />
