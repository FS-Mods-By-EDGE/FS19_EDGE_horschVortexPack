            <movingPart node="rope1" referencePoint="rope1Ref" referenceFrame="rope1Ref" scaleZ="true" playSound="true"/>
            <movingPart node="rope2" referencePoint="rope2Ref" referenceFrame="rope2Ref" scaleZ="true" playSound="true"/>
        </movingParts>
        <sounds>
            <hydraulic template="DEFAULT_HYDRAULIC_SOUND" linkNode="horschDragHose36_main_component1"/>
        </sounds>
    </cylindered>

    <fillUnit>
