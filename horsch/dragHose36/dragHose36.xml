    <storeData>
        <name>$l10n_horschDraghose</name>
        <specs>
            <neededPower>10</neededPower>
            <workingWidth>36.0</workingWidth>
            <combination>$l10n_horschSlurryTank21000</combination>
        </specs>
        <functions>
            <function>$l10n_function_slurrySpreaderTool</function>
        </functions>
        <image>horsch/dragHose36/store_horschDragHose36.png</image>
        <price>35000</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>HORSCH</brand>
        <category>slurryTanks</category>
        <vertexBufferMemoryUsage>3646424</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>1390752</indexBufferMemoryUsage>
        <textureMemoryUsage>11250480</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_draghose</typeDesc>
        <filename>horsch/dragHose36/dragHose36.i3d</filename>
        <size width="8.5" length="10.5" lengthOffset="3"/>
        <speedLimit value="17"/>
        <components>
            <component centerOfMass="0 0.5 0" solverIterationCount="10" mass="2000"/>
        </components>
        <schemaOverlay attacherJointPosition="0 0">
            <default name="DEFAULT_IMPLEMENT"/>
            <turnedOn name="DEFAULT_IMPLEMENT_ON"/>
            <selected name="DEFAULT_IMPLEMENT_SELECTED"/>
            <turnedOnSelected name="DEFAULT_IMPLEMENT_SELECTED_ON"/>
        </schemaOverlay>
    </base>

    <attachable allowFoldingWhileLowered="false">
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" jointType="implement" lowerRotLimitScale="0 0 0" upperRotLimitScale="0 0 0" upperRotationOffset="0" needsLowering="true" allowsLowering="true">
                <distanceToGround lower="0.96" upper="0.96"/>
            </inputAttacherJoint>
        </inputAttacherJoints>
        <support animationName="moveSupport"/>
        <support animationName="moveArmForDetach" foldMinLimit="0.99" foldMaxLimit="1" delayedOnLoad="true"/>
    </attachable>

    <connectionHoses>
        <hose inputAttacherJointIndices="1" type="TOOL_CONNECTOR_TOP_RIGHT" node="topRightPipeMiddle" length="1" diameter="0.16" straighteningFactor="2" isTwoPointHose="true">
            <objectChange node="topRightPipeStart" visibilityActive="true" visibilityInactive="false" />
        </hose>
        <hose inputAttacherJointIndices="1" type="CABLE_BOUNDLE"  node="cableBoundleNode"  length="2" diameter="0.05" straighteningFactor="0.5">
            <objectChange node="detachedHose" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <localHose>
            <hose type="TOOL_CONNECTOR_TOP_RIGHT"  node="topRightPipeStart"  length="2" diameter="0.16" minCenterPointAngle="30" isWorldSpaceHose="false" isTwoPointHose="true" straighteningFactor="2"/>
            <target type="TOOL_CONNECTOR_TOP_RIGHT" node="topRightPipeMiddle" />
        </localHose>
    </connectionHoses>

    <workAreas>
        <workArea type="sprayer" functionName="processSprayerArea" disableBackwards="false" requiresGroundContact="false">
            <area startNode="areaStart" widthNode="areaWidth" heightNode="areaHeight"/>
            <folding minLimit="0" maxLimit="0.1"/>
        </workArea>
    </workAreas>

    <sprayer activateOnLowering="true">
        <usageScales workingWidth="36" scale="1" />
        <sounds>
            <work template="DEFAULT_SOWING_AIR_BLOWER" linkNode="horschDragHose36_main_component1" />
        </sounds>
    </sprayer>

    <foldable>
        <foldingParts startMoveDirection="1" foldMiddleAnimTime="0.1142" detachingMinLimit="1">
            <foldingPart animationName="folding" speedScale="1"/>
        </foldingParts>
    </foldable>

    <animations>
        <animation name="moveSupport">
            <part node="supportArms" startTime="0" endTime="0.25" startTrans="0 0.4 0" endTrans="0 0 0"/>
            <part node="upperLinkRot" startTime="0" endTime="0.5" startRot="-21 0 0" endRot="0 0 0"/>
            <part node="supportArms1" startTime="0" endTime="0.5" startTrans="7.46 0 -0.006" endTrans="7.46 -2.091 -0.006"/>
            <part node="supportArms1s" startTime="0" endTime="0.5" startTrans="-7.492 0 -0.006" endTrans="-7.492 -2.091 -0.006"/>
            <part node="supportCol3" startTime="0.4" endTime="0.45" visibility="false"/>
            <part node="supportCol3" startTime="0.45" endTime="0.5" visibility="true"/>
            <part node="supportCol" startTime="0.4" endTime="0.45" visibility="false"/>
            <part node="supportCol" startTime="0.45" endTime="0.5" visibility="true"/>
        </animation>
        <animation name="moveArmForDetach" resetOnStart="false">
            <part node="leftArm"  startTime="0" endTime="0.5" startRot="0 -90 0" endRot="0 -75 0"/>
            <part node="rightArm" startTime="0" endTime="0.5" startRot="0 90 0" endRot="0 75 0"/>
        </animation>
        <animation name="folding">
            <part node="lift"       startTime="0"  endTime="1.0" startTrans="0 0 0"  endTrans="0 0.5 0"/>
            <part node="hydraulicLift"       startTime="0"  endTime="1.0" startTrans="0 0.15 0" endTrans="0 0.39 0" />
            <part node="lift"       startTime="2.0"  endTime="7.0"  startTrans="0 0.5 0"  endTrans="0 1.45 0"/>
            <part node="hydraulicLift"       startTime="2.0"  endTime="7.0"  startTrans="0 0.39 0" endTrans="0 0.85 0" />
            <part node="leftArm"     startTime="13.0" endTime="17"   startRot="0 0 0" endRot="0 -90 0"/>
            <part node="rightArm"     startTime="13.3" endTime="17.5" startRot="0 0 0" endRot="0 90 0"/>
            <part node="leftOuterArmRot"   startTime="2"    endTime="7.0"  startRot="0 0 0"  endRot="0 89 0"/>
            <part node="leftOuterArmRot"   startTime="7.5"  endTime="12.5" startRot="0 89 0" endRot="0 179.5 0"/>
            <part node="leftOuterArm" startTime="2"    endTime="2"    startRot="0 0 0"  endRot="0 0 1"/>
            <part node="leftOuterArm" startTime="12.5" endTime="13.5" startRot="0 0 1"  endRot="0 0 0"/>
            <part node="leftOuterArmRotArm"   startTime="7.45" endTime="12.5" startRot="0 0 0"  endRot="0 91 0"/>
            <part node="leftArmHosesRotation"   startTime="7.85" endTime="12.5" startRot="0 0 0"  endRot="0 140 0"/>
            <part node="rightOuterArmRot"   startTime="2.2"  endTime="7.2"  startRot="0 0 0"   endRot="0 -89 0"/>
            <part node="rightOuterArmRot"   startTime="7.7"  endTime="12.7" startRot="0 -89 0" endRot="0 -179.5 0"/>
            <part node="rightOuterArm" startTime="2"    endTime="3"    startRot="0 0 0"   endRot="0 0 -1"/>
            <part node="rightOuterArm" startTime="12.5" endTime="13.5" startRot="0 0 -1"  endRot="0 0 0"/>
            <part node="rightOuterArmRotArm"   startTime="7.65" endTime="12.7" startRot="0 0 0"   endRot="0 -91 0"/>
            <part node="rightArmHosesRotation"   startTime="7.85" endTime="12.5" startRot="0 0 0"  endRot="0 -140 0"/>
        </animation>
    </animations>

    <cylindered>
        <movingTools>
            <movingTool node="hydraulicLift" playSound="true">
                <dependentPart node="rope1"/>
