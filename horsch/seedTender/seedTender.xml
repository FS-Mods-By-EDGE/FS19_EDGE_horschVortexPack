                </additionalObjects>
            </inputAttacherJoint>
        </inputAttacherJoints>
        <support animationName="moveSupport"/>
    </attachable>

    <animations>
        <animation name="moveSupport">
            <part node="attachers" startTime="0.98" endTime="0.99" visibility="true"/>
            <part node="attachers" startTime="0.99" endTime="1.0" visibility="false"/>
        </animation>
        <animation name="pipeAnimation">
            <part node="pipe" startTime="0" endTime="8" startRot="-0.7 0 0" endRot="-146.75 0 0"/>
            <part node="body" startTime="0" endTime="8" startRot="0 0 0" endRot="-25 0 0"/>
        </animation>
    </animations>

    <cylindered>
        <movingTools>
            <movingTool node="pipe">
                <dependentPart node="hydraulicPipe"/>
            </movingTool>
            <movingTool node="body">
                <dependentPart node="hydraulicBody"/>
            </movingTool>
            <movingTool node="doorLeft">
                <dependentPart node="doorLeftArm"/>
            </movingTool>
            <movingTool node="doorRight">
                <dependentPart node="doorRightArm"/>
            </movingTool>
        </movingTools>

        <movingParts>
            <movingPart node="hydraulicPipe" referencePoint="hydraulicPipeRef" referenceFrame="hydraulicPipeRefFrame" playSound="true">
                <translatingPart node="hydraulicPipePunch"/>
            </movingPart>
            <movingPart node="hydraulicBody" referencePoint="hydraulicBodyRef" referenceFrame="hydraulicBodyRef" playSound="true">
                <translatingPart node="hydraulicBodyPunch1"/>
                <translatingPart node="hydraulicBodyPunch2"/>
                <translatingPart node="hydraulicBodyPunch3"/>
            </movingPart>
            <movingPart node="doorLeftArm" referencePoint="doorLeftArmRef" localReferencePoint="doorLeftArmPart" referenceFrame="horschSeedTender_main_component1" playSound="true">
                <dependentPart node="doorLeftArmPart"/>
                <dependentPart node="hydraulicDoorLeft"/>
            </movingPart>
            <movingPart node="doorLeftArmPart" referencePoint="doorLeftArmRef" referenceFrame="doorLeftArmRef" playSound="true"/>
            <movingPart node="hydraulicDoorLeft" referencePoint="hydraulicDoorLeftRef" referenceFrame="hydraulicDoorRefFrame" playSound="true">
                <translatingPart node="hydraulicDoorLeftPunch"/>
            </movingPart>
            <movingPart node="doorRightArm" referencePoint="doorRightArmRef" localReferencePoint="doorRightArmPart" referenceFrame="horschSeedTender_main_component1" playSound="true">
                <dependentPart node="doorRightArmPart"/>
                <dependentPart node="hydraulicDoorRight"/>
