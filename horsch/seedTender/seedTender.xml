                <effectNode effectNode="smoke1" materialType="smoke" materialTypeId="1" alignToWorldY="true" delay="0.05"/>
            </effects>
        </dischargeNode>
        <dischargeNode node="raycastNode1" emptySpeed="200" fillUnitIndex="2" unloadInfoIndex="2">
            <raycast extraDistance="-1" maxDistance="6"/>
            <effects>
                <effectNode effectClass="MorphPositionEffect" effectNode="beltMove02" materialType="belt" materialTypeId="1" delay="0" fadeTime="0.5" speed="0.9" scrollLength="1.5" scrollSpeed="2"/>
