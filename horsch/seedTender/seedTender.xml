              <state1 translation="0.125 0 0" />
              <state2 translation="0.125 0.18 0"/>
              <state3 translation="0.125 0 0"/>
          </pipeNode>
          <pipeNode node="doorRight" translationSpeeds="0 0.2 0">
              <state1 translation="-0.125 0 0" />
              <state2 translation="-0.125 0 0"/>
              <state3 translation="-0.125 0.18 0"/>
          </pipeNode>
      </pipeNodes>
        <states num="3" unloading="2 3">
