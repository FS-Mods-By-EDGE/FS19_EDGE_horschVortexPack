                <effectNode effectClass="PipeEffect" effectNode="pipeEffect02" materialType="pipe" materialTypeId="1" maxBending="0.5" delay="0.05" shapeScaleSpread="1 1 0 0"/>
                <effectNode effectNode="smoke1" materialType="smoke" materialTypeId="1" alignToWorldY="true" delay="0.05"/>
            </effects>
        </dischargeNode>
    </dischargeable>

