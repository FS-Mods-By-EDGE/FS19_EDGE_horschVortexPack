            </unloadInfo>
        </unloadInfos>
    </fillVolume>

    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="raycastNode" emptySpeed="200" fillUnitIndex="1" unloadInfoIndex="1">
            <raycast extraDistance="-1" maxDistance="6"/>
            <effects>
                <effectNode effectClass="MorphPositionEffect" effectNode="beltMove01" materialType="belt" materialTypeId="1" delay="0" fadeTime="0.5" speed="0.9" scrollLength="1.5" scrollSpeed="2"/>
